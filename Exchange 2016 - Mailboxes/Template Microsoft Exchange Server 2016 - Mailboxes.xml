                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=25</expression>
                                    <name>{#MAILALIAS} Mailbox at 25%</name>
                                    <status>DISABLED</status>
                                    <priority>WARNING</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=50</expression>
                                    <name>{#MAILALIAS} Mailbox at 50%</name>
                                    <status>DISABLED</status>
                                    <priority>WARNING</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=75</expression>
                                    <name>{#MAILALIAS} Mailbox at 75%</name>
                                    <priority>AVERAGE</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
