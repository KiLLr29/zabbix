                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=95</expression>
                                    <name>{#MAILALIAS} Mailbox at 95% | Needs ~IMMEDIATE ATTENTION~</name>
                                    <priority>HIGH</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=100</expression>
                                    <name>{#MAILALIAS} Mailbox at 100% | ~NEVER SHOULD HAVE GOTTEN HERE~</name>
                                    <opdata>Ok, Who's at fault - Admin's will be checking the ~TICKETS!~</opdata>
                                    <priority>DISASTER</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>{#MAILALIAS} | Mailbox Quota Size</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>exchange.host[mailboxquota,{#MAILALIAS}]</key>
                            <delay>1h</delay>
                            <units>bytes</units>
                            <applications>
                                <application>
                                    <name>Exchange 2016 - Quotas</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>{#MAILALIAS} | Sent Items Size</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>exchange.host[sent,{#MAILALIAS}]</key>
                            <delay>1h</delay>
                            <units>bytes</units>
                            <applications>
                                <application>
                                    <name>Exchange 2016 - Mailbox Folders</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>{#MAILALIAS} - Mailbox Info</name>
                            <show_work_period>NO</show_work_period>
                            <show_triggers>NO</show_triggers>
                            <graph_items>
                                <graph_item>
                                    <color>7E57C2</color>
                                    <calc_fnc>ALL</calc_fnc>
