                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>{#MAILALIAS} | Mailbox % Used</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>exchange.host[mailboxpercentage{#MAILALIAS}]</key>
                            <delay>1h</delay>
