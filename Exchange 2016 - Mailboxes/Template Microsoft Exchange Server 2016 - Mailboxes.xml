<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2019-11-17T21:05:19Z</date>
    <groups>
        <group>
            <name>Exchange Servers</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Microsoft Exchange Server 2016 - Mailboxes</template>
            <name>Template Microsoft Exchange Server 2016 - Mailboxes</name>
            <description>Template pulls all user mailboxes based on the ALIAS.</description>
            <groups>
                <group>
                    <name>Exchange Servers</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Exchange 2016 - Archives</name>
                </application>
                <application>
                    <name>Exchange 2016 - Calculations</name>
                </application>
                <application>
                    <name>Exchange 2016 - Mailboxes</name>
                </application>
                <application>
                    <name>Exchange 2016 - Mailbox Folders</name>
                </application>
                <application>
                    <name>Exchange 2016 - Quotas</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>Mailbox Discover</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>exchange.host[discover,mailbox]</key>
                    <delay>1h</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>{#MAILALIAS} | On-Line Archive % Used</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>exchange.host[archivepercentage{#MAILALIAS}]</key>
                            <delay>1h</delay>
                            <units>bytes</units>
                            <description>Percentage, Is used *LOOSELY* HERE</description>
                            <applications>
                                <application>
                                    <name>Exchange 2016 - Calculations</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=25</expression>
                                    <name>{#MAILALIAS} On-Line Archive at 25%</name>
                                    <status>DISABLED</status>
                                    <priority>WARNING</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=50</expression>
                                    <name>{#MAILALIAS} On-Line Archive at 50%</name>
                                    <status>DISABLED</status>
                                    <priority>WARNING</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=75</expression>
                                    <name>{#MAILALIAS} On-Line Archive at 75%</name>
                                    <priority>AVERAGE</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=85</expression>
                                    <name>{#MAILALIAS} On-Line Archive at 85%</name>
                                    <priority>HIGH</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=95</expression>
                                    <name>{#MAILALIAS} On-Line Archive at 95% | Needs ~IMMEDIATE ATTENTION~</name>
                                    <priority>HIGH</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=100</expression>
                                    <name>{#MAILALIAS} On-Line Archive at 100% | ~NEVER SHOULD HAVE GOTTEN HERE~</name>
                                    <opdata>Ok, Who's at fault - Admin's will be checking the ~TICKETS!~</opdata>
                                    <priority>DISASTER</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{change()}&gt;2</expression>
                                    <name>{#MAILALIAS} | 2% Abnormal On-Line Archive Growth Since Last Check</name>
                                    <priority>WARNING</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>{#MAILALIAS} | On-Line Archive Quota Size</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>exchange.host[archivequota,{#MAILALIAS}]</key>
                            <delay>1h</delay>
                            <units>bytes</units>
                            <applications>
                                <application>
                                    <name>Exchange 2016 - Quotas</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
